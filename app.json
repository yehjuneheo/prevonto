{
  "expo": {
    "name": "Prevonto",
    "slug": "prevonto",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.prevonto.app",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.anonymous.prevonto"
    },
    "web": {
      "favicon": ""
    },
    "extra": {
      "eas": {
        "projectId": "3de2e1a4-0ec1-494d-a6d0-17e2604521a7"
      }
    }
  }
}
